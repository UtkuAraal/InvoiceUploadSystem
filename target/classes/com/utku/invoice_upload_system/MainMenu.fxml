<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.utku.invoice_upload_system.controller.MainMenuController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="create_invoice" layoutX="100.0" layoutY="42.0" mnemonicParsing="false" onAction="#createInvoice" prefHeight="36.0" prefWidth="250.0" text="Fatura Oluştur" />
                <Button layoutX="100.0" layoutY="104.0" mnemonicParsing="false" onAction="#xmlInvoice" prefHeight="36.0" prefWidth="250.0" text="Fatura XML İşlemleri" fx:id="xml_invoice" />
                <Button fx:id="json_invoice" layoutX="100.0" layoutY="162.0" mnemonicParsing="false" onAction="#jsonInvoice" prefHeight="36.0" prefWidth="250.0" text="Fatura JSON İşlemleri" />
                <Button fx:id="delete_invoice" layoutX="100.0" layoutY="217.0" mnemonicParsing="false" onAction="#deleteInvoice" prefHeight="36.0" prefWidth="250.0" text="Fatura Sil" />
                <Button fx:id="exit" layoutX="100.0" layoutY="272.0" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="250.0" text="Çıkış" />
            </children>
        </AnchorPane>
    </children>
</VBox>
